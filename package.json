{
  "name": "targets",
  "version": "1.0.6",
  "description": "Node.js CLI bootstrapper powered by minimist and inquirer",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint .",
    "test": "./node_modules/.bin/mocha \"test/**/*.spec.js\"",
    "watch": "npm run test -- --watch",
    "coverage": "rm -rf ./coverage && ./node_modules/.bin/nyc npm test",
    "check-coverage": "npm run coverage && ./node_modules/.bin/nyc check-coverage --root=\"./coverage\" --statement 60 --function 75 --branch 40"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/machellerogden/targets.git"
  },
  "author": {
    "name": "Mac Heller-Ogden",
    "email": "mac@machellerogden.com"
  },
  "license": "MIT",
  "dependencies": {
    "answers": "^1.10.1",
    "axios": "0.16.1",
    "blessed": "^0.1.81",
    "blessed-xterm": "^1.1.19",
    "byline": "^5.0.0",
    "callsites": "^2.0.0",
    "chalk": "1.1.3",
    "globby": "^8.0.1",
    "inquirer": "^6.0.0",
    "js-yaml": "^3.12.0",
    "lodash": "^4.17.10",
    "mem": "^3.0.1",
    "stream-line-wrapper": "0.1.1",
    "stream-to-promise": "^2.2.0",
    "vm2": "^3.6.2"
  },
  "devDependencies": {
    "bytes": "2.5.0",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "babel-eslint": "^8.2.6",
    "eslint": "5.1.0",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "pre-commit": "1.2.2",
    "proxyquire": "1.7.11",
    "require-dir": "0.3.2",
    "sinon": "2.2.0",
    "sinon-chai": "2.10.0",
    "weather-js": "2.0.0"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "engines": {
    "node": ">=10.6.0"
  }
}
